		"publishPackage": "npm run build && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CaspianTechnologies/ionic-native-http-angular-wrapper.git"
	},
	"keywords": [
		"ionic",
		"angular/http",
		"cors",
		"wkwebview",
		"ionic-native"
	],
	"author": "Ganey",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ganey/ionic-native-http-angular-wrapper/issues"
	},
	"homepage": "https://github.com/ganey/ionic-native-http-angular-wrapper#readme",
	"devDependencies": {
		"@angular/common": "^4.4.0",
		"@angular/compiler": "^4.4.0",
		"@angular/compiler-cli": "^4.4.0",
		"@angular/core": "^4.4.0",
		"@angular/http": "^4.4.0",
		"@angular/platform-browser": "^4.4.0",
		"@angular/platform-browser-dynamic": "^4.4.0",
		"@ionic-native/http": "4.10.0",
		"@types/node": "^6.0.60",
		"rxjs": "^6.1.0",
		"rxjs-compat": "^6.2.0",
		"shx": "^0.3.2",
		"typescript": "^2.3.4",
		"zone.js": "^0.8.10"
	},
	"dependencies": {
		"@ionic-native/core": "4.10.0"
	}
