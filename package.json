{
	"name": "ionic-native-http-angular-wrapper",
	"version": "1.0.2",
	"description": "A basic package for making @ionic-native/http behave with observables and fall back to @angular/http.",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"prepare": "npm run build",
		"build": "shx rm -rf aot dist && npm run ngc",
